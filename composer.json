{
    "name": "anvari182/laravel-tracking-more",
    "description": "A Laravel package for using TrackingMore API.",
    "keywords": [
        "trackingMore",
        "tracking-more",
        "laravel-tracking-more",
        "laravel"
    ],
    "type": "library",
    "require": {
        "php": "^8.0",
        "illuminate/support": "^9.45"
    },
    "require-dev": {
        "cerbero/laravel-dto": "^2.2",
        "guzzlehttp/guzzle": "^7.5",
        "laravel/pint": "^1.3",
        "nunomaduro/collision": "^6.3",
        "orchestra/testbench": "^7.17",
        "pestphp/pest": "^1.22",
        "phpstan/phpstan": "^1.9",
        "phpunit/phpunit": "^9.5"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Anvari182\\TrackingMore\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Anvari182\\TrackingMore\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "anvari182",
            "email": "anvari182@gmail.com"
        }
    ],
    "scripts": {
        "analyse": "./vendor/bin/phpstan analyse",
        "test": "./vendor/bin/pest",
        "fix": "./vendor/bin/pint -v",
        "pint": "./vendor/bin/pint -v --test"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "TrackingMoreServiceProvider"
            ],
            "aliases": {
                "TrackingMore": "Anvari182\\LaravelTrackingMore\\Facades\\TrackingMore"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
